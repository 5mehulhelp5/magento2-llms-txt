        <arguments>
            <argument name="dependenciesArray" xsi:type="array">
                <item name="feedCollection" xsi:type="string">Mirasvit\Feed\Model\ResourceModel\Feed\Collection</item>
                <item name="posCollection" xsi:type="string">SR\PointOfSale\Model\ResourceModel\PointOfSale\Collection</item>
